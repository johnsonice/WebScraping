Dialogue: 0,1:33:36.26,1:33:38.47,Default,,0,0,0,,# 按地毯年龄算你有多少岁了 #\N{\fs27.401\b0}# What age are you in carpet years #
Dialogue: 0,1:33:38.47,1:33:40.68,Default,,0,0,0,,# 已经春天了么 你是第一个苏醒的么＃\N{\fs27.401\b0}# Is it spring and you are the first to wake #
Dialogue: 0,1:33:40.72,1:33:45.43,Default,,0,0,0,,# 或许虽然你已年长 但依然能抵抗寒冬 #\N{\fs27.401\b0}# Or are you old but still strong enough to keep what winter wants to take #
Dialogue: 0,1:33:45.56,1:33:51.94,Default,,0,0,0,,# 独自立着的小线头 #\N{\fs27.401\b0}# Lone standing tuft #
Dialogue: 0,1:33:55.53,1:34:02.20,Default,,0,0,0,,# 对抗着我的踩踏 #\N{\fs27.401\b0}# Defy the foot #
Dialogue: 0,1:34:05.16,1:34:09.58,Default,,0,0,0,,# 你是否能依然幸运的立着 依然没有被压倒呢 #\N{\fs27.401\b0}# Is it luck that you are still standing and you've not been flattened too #
Dialogue: 0,1:34:09.75,1:34:14.30,Default,,0,0,0,,# 在我踩上去的时候 也只是在微风吹拂下微微颤抖着 #\N{{\fs27.401\b0}# Do you tremble in the little wind that squeezed out by my shoe #
Dialogue: 0,1:34:14.55,1:34:21.30,Default,,0,0,0,,# 独自立着的小线头 #\N{\fs27.401\b0}# Lone standing tuft #
Dialogue: 0,1:34:24.31,1:34:31.06,Default,,0,0,0,,# 对抗着我的踩踏 #\N{\fs27.401\b0}# Defy the foot #
Dialogue: 0,1:34:34.02,1:34:38.36,Default,,0,0,0,,# 你小线头强大的精神 是不是钢铁造的 #\N{\fs27.401\b0}# As you tuftiness mutated soul your strands are made of steel #
Dialogue: 0,1:34:38.36,1:34:43.32,Default,,0,0,0,,# 是单单毅力就能让你免于被压倒在鞋跟之下么 #\N{\fs27.401\b0}# Is it sheer determination you've never been beneath the heel #
Dialogue: 0,1:34:43.32,1:34:49.75,Default,,0,0,0,,# 独自立着的小线头 #\N{\fs27.401\b0}# Lone standing tuft #
Dialogue: 0,1:34:53.17,1:34:59.26,Default,,0,0,0,,# 对抗着我的踩踏 #\N{\fs27.401\b0}# Defy the foot #
